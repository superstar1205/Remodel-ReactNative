require('./buildTests.js').build(true, function(error, stdout, stderr) {
  console.log('stdout: ' + stdout);
  console.log('stderr: ' + stderr);
  if (error !== null) {
    console.log('exec error: ' + error);
    exitCode = 1;
  } else {
    exitCode = 0;
  }
});
